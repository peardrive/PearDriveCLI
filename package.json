{
  "name": "@peardrive/cli",
  "main": "src/index.js",
  "version": "3.0.0",
  "author": "PearDrive Team",
  "type": "module",
  "license": "AGPL-3.0-only",
  "pear": {
    "name": "peardrive-cli",
    "type": "terminal"
  },
  "bin": {
    "peardrivecli": "dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "pear run .",
    "dev": "pear run -d .",
    "test": "brittle test/*.test.js",
    "lint": "eslint",
    "build": "cp -R src/. dist/"
  },
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "fs/*": {
      "bare": "bare-fs/*",
      "default": "fs/*"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "path/*": {
      "bare": "bare-path/*",
      "default": "path/*"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    },
    "process/*": {
      "bare": "bare-process/*",
      "default": "process/*"
    },
    "readline": {
      "bare": "bare-readline",
      "default": "readline"
    },
    "readline/*": {
      "bare": "bare-readline/*",
      "default": "readline/*"
    },
    "tty": {
      "bare": "bare-tty",
      "default": "tty"
    },
    "tty/*": {
      "bare": "bare-tty/*",
      "default": "tty/*"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "os/*": {
      "bare": "bare-os/*",
      "default": "os/*"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.9.0",
    "brittle": "^3.17.1",
    "eslint": "^9.33.0",
    "pear-interface": "^1.1.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@peardrive/core": "^2.0.3",
    "bare-fs": "^4.0.1",
    "bare-os": "^3.6.2",
    "bare-path": "^3.0.0",
    "bare-process": "^4.0.0",
    "bare-readline": "^1.0.9",
    "bare-tty": "^5.0.2",
    "barely-colours": "^1.0.0",
    "chalk": "^5.5.0",
    "qrcode-terminal": "^0.12.0",
    "ready-resource": "^1.2.0"
  }
}
