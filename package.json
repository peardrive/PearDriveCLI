{
  "name": "@peardrive/cli",
  "main": "src/index.js",
  "version": "1.0.1",
  "author": "Hope <jennabaudelaire@pm.me>",
  "pear": {
    "name": "tmp",
    "type": "terminal"
  },
  "type": "module",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "pear run .",
    "dev": "pear run -d .",
    "test": "brittle test/*.test.js",
    "lint": "eslint"
  },
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "fs/*": {
      "bare": "bare-fs/*",
      "default": "fs/*"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "path/*": {
      "bare": "bare-path/*",
      "default": "path/*"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    },
    "process/*": {
      "bare": "bare-process/*",
      "default": "process/*"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "brittle": "^3.17.1",
    "eslint": "^9.33.0",
    "pear-interface": "^1.1.0"
  },
  "dependencies": {
    "@hopets/pear-core": "^1.2.1",
    "bare-fs": "^4.0.1",
    "bare-path": "^3.0.0",
    "bare-process": "^4.0.0",
    "bare-readline": "^1.0.9",
    "bare-tty": "^5.0.2",
    "barely-colours": "^1.0.0",
    "chalk": "^5.5.0",
    "qrcode-terminal": "^0.12.0"
  }
}
